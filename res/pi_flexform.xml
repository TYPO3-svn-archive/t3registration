<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <meta>
    <langDisable>1</langDisable>
    <!--  <langChildren>1</langChildren>!-->
        </meta>
<sheets>
    <sDEF>
    <ROOT>
        <TCEforms>
            <sheetTitle>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.sheet_general</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
        <displayMode>
            <TCEforms>
                <label>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.displayMode</label>
                <config>
                    <type>select</type>
                    <items type="array">
                    <numIndex index="0" type="array">
                        <numIndex index="0">LLL:EXT:t3registration/locallang_db.xml:pi_flexform.displayMode.default</numIndex>
                        <numIndex index="1">default</numIndex>
                    </numIndex>
                    <numIndex index="1" type="array">
                        <numIndex index="0">LLL:EXT:t3registration/locallang_db.xml:pi_flexform.displayMode.subscription</numIndex>
                        <numIndex index="1">subscription</numIndex>
                    </numIndex>
                    <numIndex index="2" type="array">
                        <numIndex index="0">LLL:EXT:t3registration/locallang_db.xml:pi_flexform.displayMode.update</numIndex>
                        <numIndex index="1">update</numIndex>
                    </numIndex>
                    <numIndex index="3" type="array">
                        <numIndex index="0">LLL:EXT:t3registration/locallang_db.xml:pi_flexform.displayMode.confirmation</numIndex>
                        <numIndex index="1">confirm</numIndex>
                    </numIndex>
                    <numIndex index="4" type="array">
                        <numIndex index="0">LLL:EXT:t3registration/locallang_db.xml:pi_flexform.displayMode.delete</numIndex>
                        <numIndex index="1">delete</numIndex>
                    </numIndex>
                    <numIndex index="5" type="array">
                        <numIndex index="0">LLL:EXT:t3registration/locallang_db.xml:pi_flexform.displayMode.login</numIndex>
                        <numIndex index="1">login</numIndex>
                    </numIndex>
                    <numIndex index="6" type="array">
                        <numIndex index="0">LLL:EXT:t3registration/locallang_db.xml:pi_flexform.displayMode.forgetPassword</numIndex>
                        <numIndex index="1">forgetPassword</numIndex>
                    </numIndex>
                    </items>
                    <maxitems>100</maxitems>
                    <size>7</size>
                    <multiple>1</multiple>
                    <selectedListStyle>width:150px</selectedListStyle>
                    <itemListStyle>width:150px</itemListStyle>
                </config>
            </TCEforms>
        </displayMode>
        <pages>
            <TCEforms>
            <exclude>1</exclude>
            <label>LLL:EXT:lang/locallang_general.php:LGL.startingpoint</label>
            <config>
                <type>group</type>
                <internal_type>db</internal_type>
                <allowed>pages</allowed>
                <size>3</size>
                <maxitems>22</maxitems>
                <minitems>0</minitems>
                <show_thumbs>1</show_thumbs>
            </config>
            </TCEforms>
        </pages>

        <recursive>
            <TCEforms>
            <label>LLL:EXT:lang/locallang_general.php:LGL.recursive</label>
                <config>
                    <type>select</type>
                    <items type="array">
                        <numIndex index="0" type="array">
                            <numIndex index="0"></numIndex>
                            <numIndex index="1"></numIndex>
                        </numIndex>
                        <numIndex index="1" type="array">
                            <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.1</numIndex>
                            <numIndex index="1">1</numIndex>
                        </numIndex>
                        <numIndex index="2" type="array">
                            <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.2</numIndex>
                            <numIndex index="1">2</numIndex>
                        </numIndex>
                        <numIndex index="3" type="array">
                            <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.3</numIndex>
                            <numIndex index="1">3</numIndex>
                        </numIndex>
                        <numIndex index="4" type="array">
                            <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.4</numIndex>
                            <numIndex index="1">4</numIndex>
                        </numIndex>
                        <numIndex index="5" type="array">
                            <numIndex index="0">LLL:EXT:cms/locallang_ttc.php:recursive.I.5</numIndex>
                            <numIndex index="1">250</numIndex>
                        </numIndex>
                    </items>
                    <minitems>0</minitems>
                    <maxitems>1</maxitems>
                    <size>1</size>
                </config>
            </TCEforms>
        </recursive>
        </el>
    </ROOT>
    </sDEF>
    <s_template>
    <ROOT>
        <TCEforms>
            <sheetTitle>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.sheet_template</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
        <templateFile>
            <TCEforms>
                <label>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.template.templateFile</label>
                <config>
                    <type>group</type>
                    <internal_type>file</internal_type>
                    <allowed>tmpl,html,htm</allowed>
                    <max_size>100</max_size>
                    <uploadfolder>uploads/tx_t3registration</uploadfolder>
                    <maxitems>1</maxitems>
                    <size>1</size>
                    <selectedListStyle>Width:180px</selectedListStyle>
                </config>
            </TCEforms>
        </templateFile>
    </el>
    </ROOT>
    </s_template>
    <s_fields>
    <ROOT>
        <TCEforms>
            <sheetTitle>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.sheet_fields</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
          <fieldsManager>
            <TCEforms>
                <label>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.fieldsManager</label>
                <config>
                    <type>user</type>
                    <userFunc>EXT:t3registration/library/classes/class.tx_t3registration_piFlexForm_fieldsManagement.php:tx_t3registration_piFlexForm_fieldsManagement->getFields</userFunc>
                </config>
                </TCEforms>
          </fieldsManager>
            <useEmailAsUsername>
                <TCEforms>
                <label>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.useEmailAsUsername</label>
                <config>
                    <type>check</type>
                </config>
                </TCEforms>
            </useEmailAsUsername>
        </el>
    </ROOT>
    </s_fields>
    <s_configuration>
    <ROOT>
        <TCEforms>
            <sheetTitle>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.sheet_configuration</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
        <confirmationPage>
            <TCEforms>
            <exclude>1</exclude>
            <label>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.confirmationPage</label>
            <config>
                <type>group</type>
                <internal_type>db</internal_type>
                <allowed>pages</allowed>
                <size>1</size>
                <maxitems>1</maxitems>
                <minitems>0</minitems>
                <show_thumbs>1</show_thumbs>
            </config>
            </TCEforms>
        </confirmationPage>
          <groupAssociationMode>
                <TCEforms>
                <label>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.groupAssociationMode</label>
                <config>
                    <type>input</type>
                    <eval>alfanum,trim</eval>
                    <checkbox></checkbox>
                    <size>15</size>
                </config>
            </TCEforms>
          </groupAssociationMode>
        <emailFormat>
            <TCEforms>
                <label>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.emailFormatInHTML</label>
                <config>
                    <type>check</type>
                    <default>1</default>
                </config>
            </TCEforms>
        </emailFormat>
        <confirmationProcessMode>
            <TCEforms>
            <label>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.confirmationProcessMode</label>
                <config>
                    <type>select</type>
                    <items type="array">
                        <numIndex index="0" type="array">
                            <numIndex index="0">LLL:EXT:t3registration/locallang_db.xml:pi_flexform.confirmationProcessMode.none</numIndex>
                            <numIndex index="1">0</numIndex>
                        </numIndex>
                        <numIndex index="1" type="array">
                            <numIndex index="0">LLL:EXT:t3registration/locallang_db.xml:pi_flexform.confirmationProcessMode.user</numIndex>
                            <numIndex index="1">1</numIndex>
                        </numIndex>
                        <numIndex index="2" type="array">
                            <numIndex index="0">LLL:EXT:t3registration/locallang_db.xml:pi_flexform.confirmationProcessMode.admin</numIndex>
                            <numIndex index="1">2</numIndex>
                        </numIndex>
                    </items>
                    <minitems>0</minitems>
                    <maxitems>1</maxitems>
                    <size>1</size>
                </config>
            </TCEforms>
        </confirmationProcessMode>
        <senderEmail>
            <TCEforms>
                <label>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.senderEmail</label>
                <config>
                    <type>input</type>
                    <size>15</size>
                </config>
            </TCEforms>
        </senderEmail>
        <senderName>
            <TCEforms>
                <label>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.senderName</label>
                <config>
                    <type>input</type>
                    <size>15</size>
                </config>
            </TCEforms>
        </senderName>
        <adminEmail>
            <TCEforms>
                <label>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.adminEmail</label>
                <config>
                    <type>input</type>
                    <size>15</size>
                </config>
            </TCEforms>
        </adminEmail>
        <groupOnConfirmation>
            <TCEforms>
                <label>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.groupOnConfirmation</label>
                <config>
                    <type>select</type>
                    <foreign_table>fe_groups</foreign_table>
                    <foreign_table_where>ORDER BY title</foreign_table_where>
                    <size>7</size>
                    <maxitems>70</maxitems>
                </config>
            </TCEforms>
        </groupOnConfirmation>
        <groupOnRegistration>
            <TCEforms>
                <label>LLL:EXT:t3registration/locallang_db.xml:pi_flexform.groupOnRegistration</label>
                <config>
                    <type>select</type>
                    <foreign_table>fe_groups</foreign_table>
                    <foreign_table_where>ORDER BY title</foreign_table_where>
                    <size>7</size>
                    <maxitems>70</maxitems>
                </config>
            </TCEforms>
        </groupOnRegistration>
        </el>
    </ROOT>
    </s_configuration>
</sheets>
</T3DataStructure>
